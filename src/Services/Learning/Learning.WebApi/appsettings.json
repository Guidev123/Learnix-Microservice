{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": ""
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Learnix.Learning.WebApi"
    }
  },
  "Authentication": {
    "Audience": "account",
    "TokenValidationParameters": {
      "ValidIssuers": [ "" ]
    },
    "MetadataAddress": "",
    "RequireHttpsMetadata": true
  },
  "KeycloakExtensions": {
    "HealthUrl": "",
    "AuthorizationUrl": "",
    "TokenUrl": ""
  },
  "ConnectionStrings": {
    "Database": "",
    "Cache": ""
  },
  "MessageBusOptions": {
    "BootstrapServer": "",
    "GroupId": ""
  },
  "OutboxOptions": {
    "IntervalInSeconds": 5,
    "BatchSize": 20
  },
  "InboxOptions": {
    "IntervalInSeconds": 5,
    "BatchSize": 20
  },
  "ExternalServices": {
    "UsersApi": ""
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": ""
}